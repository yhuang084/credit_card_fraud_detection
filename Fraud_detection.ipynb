    "#define a helper function to aggregate machine learning models and their corresponding recall scores\n",
    "def evaluate(X_train, X_test, y_train, y_test):\n",
    "    log = LogisticRegression().fit(X_train, y_train)\n",
    "    dt = DecisionTreeClassifier().fit(X_train, y_train)\n",
    "    rf = RandomForestClassifier().fit(X_train, y_train)\n",
    "    nb =GaussianNB().fit(X_train, y_train)\n",
    "    xgb = XGBClassifier().fit(X_train, y_train)\n",
    "\n",
    "    log_recall = recall_score(log.predict(X_test), y_test)\n",
    "    dt_recall = recall_score(dt.predict(X_test), y_test)\n",
    "    rf_recall = recall_score(rf.predict(X_test), y_test)\n",
    "    nb_recall = recall_score(nb.predict(X_test), y_test)\n",
    "    xgb_recall = recall_score(xgb.predict(X_test), y_test)\n",
    "\n",
    "    print(f\"Logistic regression: {log_recall} \\nDecision tree: {dt_recall} \\nRandom forest: {rf_recall} \\nNaive Bayes: {nb_recall} \\nXGBoosting: {xgb_recall}\")\n",
