    "majority_class = df[df['Class'] == 0]\n",
    "minority_class = df[df['Class'] == 1]\n",
    "\n",
    "##downsample the majority class\n",
    "majority_downsampled = resample(majority_class, replace = True, n_samples=len(minority_class), random_state=42)\n",
    "\n",
    "df_downsampled = pd.concat([minority_class, majority_downsampled]).reset_index(drop = True)\n",
    "\n",
    "df_downsampled['Class'].value_counts()"
